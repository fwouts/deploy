{
  "name": "@zenclabs/deploy",
  "version": "0.0.6",
  "author": "Francois Wouts <f@zenc.io>",
  "license": "MIT",
  "description": "Deploy is a command-line tool that aims to make Docker deployments as simple as possible.",
  "keywords": [
    "AWS",
    "Docker"
  ],
  "homepage": "https://github.com/zenclabs/deploy-cli",
  "bugs": "https://github.com/zenclabs/deploy-cli/issues",
  "bin": {
    "deploy": "./dist/main.js"
  },
  "scripts": {
    "dev": "nodemon -e ts -w src/ --exec ts-node src/main.ts",
    "build": "rimraf dist/ && webpack && echo \"#!/usr/bin/env node\\n\" | cat - dist/main.js > dist/main-with-prefix.js && mv dist/main-with-prefix.js dist/main.js && chmod +x dist/main.js"
  },
  "dependencies": {
    "atob": "^2.0.3",
    "aws-sdk": "^2.169.0",
    "axios": "^0.17.1",
    "btoa": "^1.1.2",
    "chalk": "^2.3.0",
    "cloudformation-declarations": "0.0.9",
    "commander": "^2.12.2",
    "dockerode": "^2.5.3",
    "lodash": "^4.17.4",
    "portfinder": "^1.0.13",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/dockerode": "^2.5.1",
    "@types/lodash": "^4.14.89",
    "@types/node": "^8.5.1",
    "nodemon": "^1.12.7",
    "rimraf": "^2.6.2",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.0.1",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.3",
    "webpack": "^3.10.0"
  }
}
